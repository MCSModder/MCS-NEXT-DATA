        },
        {
          "ItemID": 384,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 5938,
          "AvatarBindKey": "",
          "StoryText": "没错，道友能来到此地也是你我缘分一场。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 386,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "眼前的傀儡突然发声。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 385,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "！！{vpunch=10,0.5}",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
