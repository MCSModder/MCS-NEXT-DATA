        {
          "ItemID": 281,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "澹台仙向你作了一揖，继续和执事讨论解决之法去了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 280,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "",
          "StoryText": "可惜...可惜...",
          "Description": "",
