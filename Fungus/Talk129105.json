        {
          "ItemID": 762,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "AvatarXinghe",
          "StoryText": "真是不好意思，让{$daoyou}听了我这么多抱怨。",
          "Description": "",
