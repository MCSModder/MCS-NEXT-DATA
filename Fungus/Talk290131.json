      "Position": "(-3010.843,-1387.278)",
      "Commands": [
        {
          "ItemID": 353,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "AvatarPengsha",
          "StoryText": "{$daoyou}又有何事啊？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 407,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "AvatarPengsha",
          "StoryText": "刚才那位和你争吵的女修士，她要的蓬莎巨蟹螯你这儿还有吗？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 411,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "AvatarPengsha",
          "StoryText": "今天可真是怪了，怎么现在你也要这玩意儿。但是说好了，我这儿可不能便宜卖给你，价格还是八千多灵石。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
